<script>
	//单独页面上的js
	$('.danx label').click(function(){
		$(this).toggleClass('on')
	})
	$('.us2_t span').click(function(){
		var num=$(this).index();
		$(this).siblings().removeClass('on');
		$(this).addClass('on');
		$('.us_b ul').removeClass('on');
		$('.us_b ul').eq(num).addClass('on');
	})
</script>